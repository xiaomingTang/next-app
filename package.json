{
  "$schema": "https://json.schemastore.org/package",
  "name": "next-app",
  "version": "0.1.0",
  "packageManager": "pnpm@8.14.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx prisma generate",
    "prepare": "husky install && bash -c 'chmod ug+x ./deploy.sh' && bash -c 'chmod ug+x .husky/*'",
    "beforeRun": "cross-env NEXT_PUBLIC_APP_ENV=production tsx bin/pre-run.ts",
    "predev": "pnpm run beforeRun",
    "dev": "next dev",
    "prebuild": "rimraf .next out && pnpm run beforeRun",
    "build": "next build",
    "prestart": "pnpm run beforeRun",
    "start": "cd out && node server.js",
    "preserver": "pnpm run beforeRun",
    "server": "node server.js",
    "predeploy": "bash -c 'chmod ug+x ./deploy.sh'",
    "deploy": "./deploy.sh",
    "export": "tsx bin/prepare-output.ts",
    "commit": "git-cz",
    "lint": "next lint && tsc --noEmit",
    "pretest": "pnpm run beforeRun",
    "test": "next lint && tsc --noEmit",
    "lint:commit": "commitlint -e",
    "lint-staged": "lint-staged --quiet",
    "format": "prettier --write .",
    "svg": "tsx bin/generate-svg-interface.ts",
    "db:push": "dotenv -e .env -c -- prisma db push",
    "db:seed": "dotenv -e .env -c -- prisma db seed"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm run lint-staged",
      "pre-push": "pnpm run lint",
      "commit-msg": "pnpm run lint:commit"
    }
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prisma": {
    "seed": "tsx bin/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.515.0",
    "@aws-sdk/s3-request-presigner": "^3.515.0",
    "@aws-sdk/signature-v4-crt": "^3.515.0",
    "@ebay/nice-modal-react": "^1.2.13",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hapi/boom": "^10.0.1",
    "@mui/icons-material": "^5.15.10",
    "@mui/lab": "5.0.0-alpha.165",
    "@mui/material": "^5.15.10",
    "@mui/material-nextjs": "^5.15.11",
    "@prisma/client": "^5.10.2",
    "@sinclair/typebox": "^0.32.14",
    "@zimi/utils": "^0.17.0",
    "autoprefixer": "10.4.17",
    "case": "^1.6.3",
    "client-only": "^0.0.1",
    "clsx": "^2.1.0",
    "cookie": "^0.6.0",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "image-size": "^1.1.1",
    "jsonschema": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jsqr": "^1.4.0",
    "lodash-es": "^4.17.21",
    "material-ui-colors": "^1.0.0",
    "mime-types": "^2.1.35",
    "nanoid": "^5.0.6",
    "next": "14.1.0",
    "next-auth": "^4.24.6",
    "next-mdx-remote": "^4.4.1",
    "next-pwa": "^5.6.0",
    "opentype.js": "^1.3.4",
    "postcss": "8.4.35",
    "province-city-china": "^8.5.7",
    "qrcode.react": "^3.1.0",
    "randomcolor": "^0.6.2",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-hot-toast": "^2.4.1",
    "react-photo-view": "^1.2.4",
    "react-use": "^17.5.0",
    "rehype-prism-plus": "^2.0.0",
    "remark-gfm": "^3.0.1",
    "rss": "^1.2.2",
    "sass": "^1.71.1",
    "server-only": "^0.0.1",
    "sharp": "^0.33.2",
    "showdown": "^2.1.0",
    "stackedit-js": "^1.0.7",
    "swr": "^2.2.5",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@next/bundle-analyzer": "^14.1.0",
    "@svgr/webpack": "^8.1.0",
    "@types/cookie": "^0.6.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/mime-types": "^2.1.4",
    "@types/node": "20.11.20",
    "@types/opentype.js": "^1.3.8",
    "@types/randomcolor": "^0.5.9",
    "@types/react": "18.2.58",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "18.2.19",
    "@types/rss": "^0.0.32",
    "@types/showdown": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@zimi/type-utils": "^0.17.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "loader-utils": "^3.2.1",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "raw-loader": "^4.0.2",
    "tsx": "^4.7.1"
  }
}
