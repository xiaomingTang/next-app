{
  "$schema": "https://json.schemastore.org/package",
  "name": "next-app",
  "version": "0.1.0",
  "packageManager": "pnpm@8.6.9",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "bash -c 'chmod ug+x ./deploy.sh' && husky install && is-ci || bash -c 'chmod ug+x .husky/*'",
    "beforeRun": "node bin/runtime-scripts/pre-run.js",
    "predev": "pnpm run beforeRun",
    "dev": "next dev",
    "prebuild": "rimraf .next out && prisma generate && cross-env NEXT_PUBLIC_APP_ENV=production pnpm run beforeRun",
    "build": "cross-env NEXT_PUBLIC_APP_ENV=production next build",
    "prestart": "pnpm run beforeRun",
    "start": "cd out && node server.js",
    "server": "node server.js",
    "deploy": "bash -c 'chmod ug+x ./deploy.sh' && ./deploy.sh",
    "export": "ts-node --project tsconfig-for-bin.json -r tsconfig-paths/register bin/prepare-output.ts",
    "commit": "git-cz",
    "lint": "next lint && tsc --noEmit",
    "test": "prisma generate && cross-env NEXT_PUBLIC_APP_ENV=production pnpm run beforeRun && next lint && tsc --noEmit",
    "lint:commit": "commitlint -e",
    "lint-staged": "lint-staged --quiet",
    "format": "prettier --write .",
    "svg": "cross-env NODE_ENV=production ts-node --project tsconfig-for-bin.json -r tsconfig-paths/register bin/generate-svg-interface.ts",
    "db:push": "dotenv -e .env -c -- prisma db push",
    "db:seed": "dotenv -e .env -c -- prisma db seed"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm run lint-staged",
      "pre-push": "pnpm run lint",
      "commit-msg": "pnpm run lint:commit"
    }
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prisma": {
    "seed": "ts-node --project tsconfig-for-bin.json -r tsconfig-paths/register bin/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.400.0",
    "@aws-sdk/s3-request-presigner": "^3.400.0",
    "@aws-sdk/signature-v4-crt": "^3.398.0",
    "@ebay/nice-modal-react": "^1.2.10",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hapi/boom": "^10.0.1",
    "@mui/icons-material": "^5.14.7",
    "@mui/lab": "5.0.0-alpha.142",
    "@mui/material": "^5.14.7",
    "@prisma/client": "^5.2.0",
    "@sinclair/typebox": "^0.31.8",
    "autoprefixer": "10.4.15",
    "clsx": "^2.0.0",
    "cookie": "^0.5.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "image-size": "^1.0.2",
    "jsonschema": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jsqr": "^1.4.0",
    "lodash-es": "^4.17.21",
    "material-ui-colors": "^1.0.0",
    "mime-types": "^2.1.35",
    "nanoid": "^4.0.2",
    "next": "13.4.19",
    "next-auth": "^4.23.1",
    "next-mdx-remote": "^4.4.1",
    "next-pwa": "^5.6.0",
    "pascal-case": "^3.1.2",
    "postcss": "8.4.29",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-hot-toast": "^2.4.1",
    "react-photo-view": "^1.2.3",
    "react-use": "^17.4.0",
    "rehype-prism-plus": "^1.6.3",
    "remark-gfm": "^3.0.1",
    "rss": "^1.2.2",
    "sass": "^1.66.1",
    "sharp": "^0.32.5",
    "stackedit-js": "^1.0.7",
    "swr": "^2.2.2",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@next/bundle-analyzer": "^13.4.19",
    "@svgr/webpack": "^8.1.0",
    "@types/cookie": "^0.5.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash-es": "^4.17.9",
    "@types/mime-types": "^2.1.1",
    "@types/node": "20.5.7",
    "@types/react": "18.2.21",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "18.2.7",
    "@types/rss": "^0.0.30",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@zimi/type-utils": "^0.16.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^14.0.1",
    "loader-utils": "^3.2.1",
    "prettier": "^3.0.3",
    "prisma": "^5.2.0",
    "raw-loader": "^4.0.2"
  }
}
