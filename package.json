{
  "$schema": "https://json.schemastore.org/package",
  "name": "next-app",
  "version": "0.1.0",
  "packageManager": "pnpm@10.7.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx prisma generate",
    "prepare": "husky || true",
    "dev": "concurrently -n next.js,peerjs-server \"next dev\" \"peerjs --port 4000 --key peerjs --path /peer-server\"",
    "build": "next build",
    "server": "concurrently -n next.js,peerjs-server \"node server.js\" \"peerjs --port 4000 --key peerjs --path /peer-server\"",
    "test": "eslint && tsc --noEmit",
    "lint:commit-message": "commitlint -e",
    "lint:commit-content": "lint-staged --quiet",
    "db:push": "dotenv -e .env -c -- prisma db push",
    "db:seed": "dotenv -e .env -c -- prisma db seed",
    "docker:rm-none-image": "docker rmi $(docker images --filter \"dangling=true\" -q --no-trunc) 2>/dev/null",
    "docker:build": "docker build -t 16px_cc --build-arg NEXT_PUBLIC_LAST_COMMIT_MESSAGE=\"$(git log -1 --pretty=format:'%B' HEAD | tr '\n' ' ')\" --progress plain --platform=linux/amd64 .",
    "docker:deploy": "dotenv -e .env.deploy -c -v DOCKER_IMAGE_NAME=16px_cc -- /bin/bash deploy.sh"
  },
  "lint-staged": {
    "*.{md,json,svg,js,jsx,css,less,sass,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@ebay/nice-modal-react": "^1.2.13",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@hapi/boom": "^10.0.1",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/material-nextjs": "^7.2.0",
    "@mui/x-tree-view": "^8.9.0",
    "@prisma/client": "^6.12.0",
    "@react-three/drei": "10.6.0",
    "@react-three/fiber": "9.2.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/three": "^0.178.1",
    "@use-gesture/react": "^10.3.1",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "@zimi/hooks": "^0.18.5",
    "@zimi/interact": "^0.17.3",
    "@zimi/remote": "^0.2.1",
    "@zimi/utils": "^0.18.6",
    "ansi-styles": "^6.2.1",
    "arg": "^5.0.2",
    "autoprefixer": "10.4.21",
    "client-only": "^0.0.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.0",
    "cos-js-sdk-v5": "^1.10.1",
    "cos-nodejs-sdk-v5": "2.16.0-beta.3",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.9",
    "grapheme-splitter": "^1.0.4",
    "image-size": "^2.0.2",
    "immer": "^10.1.1",
    "jsonwebtoken": "^9.0.2",
    "jsqr": "^1.4.0",
    "lodash-es": "^4.17.21",
