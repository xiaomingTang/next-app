    "prisma": "^6.12.0",
    "raw-loader": "^4.0.2",
    "schema-dts": "^1.1.5",
    "tsx": "^4.20.3",
    "vconsole": "^3.15.1"
  },
  "pnpm": {
    "patchedDependencies": {},
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "@tailwindcss/oxide",
      "core-js",
      "cos-js-sdk-v5",
      "esbuild",
      "prisma",
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "unrs-resolver"
    ]
  }
}
